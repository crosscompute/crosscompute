{% extends BASE_JINJA2 if IS_STATIC else LIVE_JINJA2 %}

{% block header_html %}
<a href="{{ ROOT_URI or '/' }}">Root</a> &gt;
<a href="{{ ROOT_URI }}{{ uri }}">{{ name }}</a>
{% endblock %}

{% block main_html %}
{% if mode_name != 'none' %}
{{ super() }}
{% include 'modeMainHtml.jinja2' %}
{% endif %}
<ul class="_batches">
{% for batch in batches %}
<li>
{{ batch['name'] }}
<a href="{{ ROOT_URI }}{{ uri }}{{ batch['uri'] }}/i">[input]</a>
<a href="{{ ROOT_URI }}{{ uri }}{{ batch['uri'] }}/o">[output]</a>
</li>
{% endfor %}
</ul>
{#
<ul class="_runs">
{% for run in runs %}
<li>
{{ run['name'] }}
<a href="{{ ROOT_URI }}{{ uri }}{{ run['uri'] }}/i">[input]</a>
<a href="{{ ROOT_URI }}{{ uri }}{{ run['uri'] }}/o">[output]</a>
</li>
{% endfor %}
</ul>
#}
{% endblock %}

{% block body_js %}
{% include 'modeBodyJs.jinja2' %}
{{ super() }}
{% endblock %}
